                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">计量单位</label>
                                                <div class="col-sm-10">
                                                    <input class="form-control" type="text" disabled v-model="unitValue">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">出货天数</label>
                                                <div class="col-sm-10">
                                                    <input class="form-control" type="text" disabled v-model="shipmentsDays">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">产地</label>
                                                <div class="col-sm-10">
                                                    <input class="form-control" type="text" disabled v-model="fromArea">
                                                </div>
